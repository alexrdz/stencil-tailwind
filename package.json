    "@stencil/router": "^1.0.1",
    "@types/autoprefixer": "^9.7.2",
    "autoprefixer": "^9.7.6",
    "postcss-import": "^12.0.1",
    "rollup-plugin-postcss": "^3.1.1",
    "tailwindcss": "^1.3.5"
  },
  "license": "MIT"
}
