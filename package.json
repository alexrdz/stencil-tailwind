{
  "name": "stencil-starter-project-name",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "export NODE_ENV=production && stencil build",
    "start": "export NODE_ENV=dev && stencil build --dev --watch --serve",
